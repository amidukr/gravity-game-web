{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "outDir": "./es-gen/",
    "sourceMap": true,
    "target": "ESNext",
    "module": "es6",
    "jsx": "react",
    "allowJs": true,
    "incremental": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true
  }
}
